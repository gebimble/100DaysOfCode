height	curses_example.py	/^height = 5$/;"	v
stdscr	curses_example.py	/^stdscr = curses.initscr()$/;"	v
tb	curses_example.py	/^tb = curses.textpad.Textbox(win)$/;"	v
text	curses_example.py	/^text = tb.edit()$/;"	v
width	curses_example.py	/^width = 40$/;"	v
win	curses_example.py	/^win = curses.newwin(height, width, begin_y, begin_x)$/;"	v
